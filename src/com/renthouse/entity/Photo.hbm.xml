<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.renthouse.entity.Photo" table="photo">
        <id name="photoId" column = "photo_id" type="java.lang.Integer">
            <generator class="native" />           <!-- 主键自增长 -->
        </id>
        <!-- 
        <property name="houseId" column = "house_id" type="java.lang.Integer">
        </property>
         -->
        
        <property name="photoUrl" column = "photo_url" type="java.lang.String">
        </property> 
        <many-to-one name="aHouse" class="com.renthouse.entity.House" column="fk_photo_house"/>
    </class>
</hibernate-mapping>