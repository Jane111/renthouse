<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.renthouse.entity.House" table="house">
        <id name="houseId" column = "house_id" type="java.lang.Integer">
            <generator class="native" />           <!-- 主键自增长 -->
        </id>
        <!-- 
         <property name="comId" column = "com_id" type="java.lang.Integer">
        </property>
        <property name="userId" column = "user_id" type="java.lang.Integer">
        </property>
        
         -->
       
        <property name="houseBuilding" column = "house_building" type="java.lang.Integer">
        </property>
        <property name="houseUnit" column = "house_unit" type="java.lang.Integer">
        </property>
        <property name="houseNum" column = "house_num" type="java.lang.Integer">
        </property>
        <property name="houseRoomNum" column = "house_room_num" type="java.lang.Integer">
        </property>
        <property name="houseHallNum" column = "house_hall_num" type="java.lang.Integer">
        </property>
        <property name="houseBathroomNum" column = "house_bathroom_num" type="java.lang.Integer">
        
        </property>
        <property name="houseArea" column = "house_area" type="java.lang.Double">
        </property>
        <property name="houseDirection" column = "house_direction" type="java.lang.String">
        </property>
        <property name="housePrice" column = "house_price" type="java.lang.Double">
        </property>
        <property name="houseFloor" column = "house_floor" type="java.lang.String">
        </property>
        <property name="houseTotalNum" column = "house_total_num" type="java.lang.String">
        </property>
        <property name="houseDecoration" column = "house_decoration" type="java.lang.String">
        </property>
        <property name="housePayType" column = "house_pay_type" type="java.lang.String">
        </property>
        <property name="houseComment" column = "house_comment" type="java.lang.String">
        </property>
        <property name="houseRentType" column = "house_rent_type" type="java.lang.String">
        </property>
        <property name="houseRoomType" column = "house_room_type" type="java.lang.String">
        </property>
        <property name="houseRoomArea" column = "house_room_area" type="java.lang.Double">
        </property>
        <property name="houseAddTime" column = "house_add_time" type="java.sql.Date">
        </property>
       <!-- 
       <set name="setCollect" cascade="save-update">
			<key column="fk_collect_house"/>
			<one-to-many class="com.renthouse.entity.Collect"/>
		</set>
		<set name="setPhoto" cascade="save-update,delete">
			<key column="fk_photo_house"/>
			<one-to-many class="com.renthouse.entity.Photo"/>
		</set>
		<set name="setQuestion" cascade="save-update,delete">
			<key column="fk_question_house"/>
			<one-to-many class="com.renthouse.entity.Question"/>
		</set>
		 -->
        
        <many-to-one name="aUser" class="com.renthouse.entity.User" column="fk_house_user"/>
        <many-to-one name="aCommunity" class="com.renthouse.entity.Community" column="fk_house_community"/>
    </class>
</hibernate-mapping>