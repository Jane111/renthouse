<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.renthouse.entity.Collect" table="collect">
        <id name="collectId" column = "collect_id" type="java.lang.Integer">
            <generator class="native" />           <!-- 主键自增长 -->
        </id>
        <!-- 
        <property name="houseId" column = "house_id" type="java.lang.Integer">
        </property>
        <property name="userId" column = "user_id" type="java.lang.Integer">
        </property>
         -->
        
        <property name="collectTime" column = "collect_time" type="java.sql.Date">
        </property>
        
        <many-to-one name="aUser" class="com.renthouse.entity.User" column="fk_collect_user"/>
         <many-to-one name="aHouse" class="com.renthouse.entity.House" column="fk_collect_house"/>
    </class>
</hibernate-mapping>