<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.renthouse.entity.Community" table="community">
        <id name="comId" column = "com_id" type="java.lang.Integer">
            <generator class="native" />           <!-- 主键自增长 -->
        </id>
        <property name="comName" column = "com_name" type="java.lang.String">
        </property>
        <property name="comArea" column = "com_area" type="java.lang.String">
        </property>
        <property name="comStreet" column = "com_street" type="java.lang.String">
        </property>
       <!--  <set name="setHouse" cascade="save-update,delete">
			<key column="fk_house_community"/>
			<one-to-many class="com.renthouse.entity.House"/>
		</set>--> 
    </class>
</hibernate-mapping>